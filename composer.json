{
    "name": "geotab/mygeotab-php",
    "type": "library",
    "description": "Unofficial PHP client for the MyGeotab API",
    "keywords": ["geotab", "mygeotab", "api", "sdk"],
    "license": "Apache2",
    "authors": [
        {
            "name": "Mike Murkovic",
            "email": "mikemurkovic@geotab.com",
            "homepage": "http://mikemurko.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
      "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Geotab\\": "src/Geotab/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geotab\\API\\Tests\\": "test/"
        }
    }
}
